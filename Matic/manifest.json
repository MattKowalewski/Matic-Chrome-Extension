{
    "manifest_version": 2,
	"name": "Matic extension",
    "description": "Open specific browser tabs regularly",
    "version": "1.0",
    "permissions": [ "storage" , "tabs" ],
    "background": {
        "scripts": [ "background.js" ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "popup.html"
    }
}